public class wkdeogusexam1 {
    public static void main(String[] args) {
        String ssn1 = "장대현 남자";

        String firstNum = ssn1.substring(0,3);
        System.out.println("이름은 : " + firstNum);

        String secondNum = ssn1.substring(4);
        System.out.println("성별은 : " + secondNum);

    }
}
