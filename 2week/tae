public class taeexam {
    int minus(int x, int y) {
        int result = x + y;
        return result;
    }

    String standup() {
        System.out.println("일어납니다.");
        return null;
    }

    String sitdown() {
        System.out.println("앉습니다.");
        return null;
    }

    double divide(int x, int y) {
        double result = (double) x / (double) y;
        return result;
    }
}
